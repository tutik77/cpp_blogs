{
    "listeners": [
        {
            "address": "0.0.0.0",
            "port": 3001,
            "https": false
        }
    ],
    "db_clients": [
        {
            "rdbms": "postgresql",
            "host": "postgres_app",
            "port": 5432,
            "dbname": "app_service",
            "user": "root",
            "passwd": "12341234",
            "is_fast": false,
            "number_of_connections": 1,
            "timeout": -1.0
        }
    ],
    "app": {
        "number_of_threads": 1,
        "enable_session": false,
        "session_timeout": 0,
        "document_root": "./",
        "upload_path": "uploads",
        "file_types": [
            "gif",
            "png",
            "jpg",
            "jpeg",
            "mp4",
            "webm",
            "mov"
        ],
        "max_connections": 100000,
        "max_connections_per_ip": 0,
        "load_dynamic_views": false,
        "log": {
            "logfile_base_name": "",
            "log_size_limit": 100000000,
            "log_level": "INFO"
        },
        "run_as_daemon": false,
        "relaunch_on_error": false,
        "use_sendfile": true,
        "use_gzip": true,
        "use_brotli": false,
        "static_files_cache_time": 5,
        "idle_connection_timeout": 60,
        "server_header_field": "",
        "enable_server_header": true,
        "enable_date_header": true,
        "keepalive_requests": 0,
        "pipelining_requests": 0,
        "gzip_static": true,
        "br_static": true,
        "client_max_body_size": "20M",
        "client_max_memory_body_size": "1M",
        "client_max_websocket_message_size": "128K"
    },
    "custom_config": {
        "auth_service_url": "http://host.docker.internal:3000",
        "jwt_secret": "secret"
    }
}